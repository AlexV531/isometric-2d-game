config_version=5

[application]

config/name="Isometric3dGame"
run/main_scene="res://world_3d.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://assets/images/icon.svg"

[autoload]

Global="*res://global.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[dialogic]

glossary/default_case_sensitive=true
variables={}
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
layout/styles={
"Default": {
"export_overrides": {
"box_size": "Vector2(1000, 320)",
"box_tilt": "0.0",
"name_label_font_size": "32.0",
"text_size": "32.0"
},
"inherits": "",
"layout": "res://addons/dialogic/Modules/DefaultLayouts/Default/DialogicDefaultLayout.tscn"
},
"New Style": {
"layout": "res://dialogic_style_testing.tscn"
},
"New Style 2": {
"export_overrides": {},
"inherits": "",
"layout": "res://addons/dialogic/Modules/DefaultLayouts/RPG_BoxPortrait/DialogicRPGLayout.tscn"
}
}
layout/default_style="res://assets/Dialogue/Main.tres"
directories/dch_directory={
"player": "res://assets/Dialogue/player.dch",
"soup": "res://assets/Dialogue/soup.dch"
}
directories/dtl_directory={
"timeline": "res://assets/Dialogue/timeline.dtl"
}
layout/style_list=["res://Assets/Dialogue/Test.tres", "res://assets/Dialogue/Test3.tres", "res://assets/Dialogue/Main.tres"]

[dialogue_manager]

general/balloon_path="res://balloon.tscn"

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/size/initial_position_type=0
window/stretch/scale=0.5
window/per_pixel_transparency/allowed=true
window/vsync/vsync_mode=0

[editor]

movie_writer/disable_vsync=true

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

